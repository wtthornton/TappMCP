{
  "mcp.servers": {
    "tappmcp-local": {
      "command": "node",
        "args": ["dist/mcp-docker-server.js", "--stdio"],
      "cwd": "C:\\cursor\\TappMCP",
      "env": {
        "NODE_ENV": "production",
        "CONTEXT7_API_KEY": "ctx7sk-45825e15-2f53-459e-8688-8c14b0604d02",
        "CONTEXT7_USE_HTTP_ONLY": "true"
      },
      "stdio": true,
      "description": "TappMCP Local Server - Full VibeTapp integration with all 7 tools"
    },
    "tappmcp-container": {
      "command": "docker",
      "args": ["exec", "-i", "tappmcp-smart-mcp-1", "node", "dist/server.js"],
      "env": {
        "NODE_ENV": "production",
        "CONTEXT7_API_KEY": "ctx7sk-45825e15-2f53-459e-8688-8c14b0604d02",
        "CONTEXT7_USE_HTTP_ONLY": "true"
      },
      "stdio": true,
      "description": "TappMCP Docker Container - Full VibeTapp integration with dashboard"
    }
  },
  "mcp.enabled": true,
  "mcp.defaultServer": "tappmcp-local",
  "mcp.logLevel": "info",
  "mcp.timeout": 30000
}
