{
  "mcp.servers": {
    "tappmcp": {
      "command": "node",
      "args": ["dist/simple-mcp-server.js"],
      "cwd": "C:\\cursor\\TappMCP",
      "env": {
        "NODE_ENV": "production"
      },
      "stdio": true
    },
    "tappmcp-container": {
      "command": "docker",
      "args": ["exec", "-i", "tappmcp-tappmcp-1", "node", "dist/simple-mcp-server.js"],
      "env": {
        "NODE_ENV": "production"
      },
      "stdio": true,
      "description": "Connect to Docker container (tappmcp-tappmcp-1)"
    }
  },
  "mcp.enabled": true,
  "mcp.defaultServer": "tappmcp"
}
